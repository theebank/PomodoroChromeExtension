{
    "manifest_version": 3,
    "name": "Pomodoro",
    "version":"1.0",
    "description": "Pomodoro Technique Timer for Chrome Extension",
    "icons":{
        "16": "images/Pomodoro.png",
        "32": "images/Pomodoro.png",
        "64": "images/Pomodoro.png"
    },
    "permissions": [
        "storage",
        "alarms",
        "notifications"
    ],
    "action":{
        "default_popup": "window/window.html",
        "default_title": "Pomodoro Timer",
        "default_icon": "images/Pomodoro.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options/options.html"

}